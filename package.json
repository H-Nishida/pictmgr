{
  "name": "pictmgr",
  "version": "1.0.0",
  "description": "Photo viewer",
  "main": "./dist/app.js",
  "scripts": {
    "postinstall": "pushd frontend; yarn install; popd",
    "build-dev": "yarn -s run webpack --mode development  --config ./webpack.config.js --context ./",
    "build": "yarn -s run webpack --mode production  --config ./webpack.config.js --context ./",
    "build-front": "pushd frontend; yarn build; popd",
    "start": "yarn -s run electron ./dist/app.js",
    "license": "yarn licenses generate-disclaimer > dist/public/oss.txt",
    "dist": "yarn build && yarn build-front && yarn license && electron-builder",
    "runpkg-mac": "./build/mac/pictmgr.app/Contents/MacOS/pictmgr"
  },
  "build": {
    "appId": "family.photo",
    "mac": {
      "category": "public.app-category.photography"
    },
    "directories": {
      "output": "build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hn999/pictmgr.git"
  },
  "keywords": [
    "Photo",
    "viewer"
  ],
  "author": "h_n",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hn999/pictmgr/issues"
  },
  "homepage": "https://github.com/hn999/pictmgr#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "exif-reader": "^1.0.3",
    "express": "^4.17.1",
    "express-password-protect": "^1.0.2",
    "glob-ext": "^1.0.2",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "image-thumbnail": "^1.0.13",
    "pako": "^2.0.3",
    "serve-index": "^1.9.1",
    "sharp": "^0.25.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^7.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-node-externals": "^2.5.2"
  }
}
