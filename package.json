{
  "name": "familyphoto",
  "version": "1.0.0",
  "description": "Photo viewer",
  "main": "main.js",
  "directories": {
    "doc": "doc"
  },
  "build": {
    "appId": "FamilyPhoto",
    "mac": {
      "category": "public.app-category.photography"
    },
    "directories": {
      "output": "dist"
    }
  },
  "scripts": {
    "start": "npx electron main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "npx electron-builder --dir",
    "dist": "npx electron-builder --publish=never",
    "postinstall": "npx electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hn999/pictmgr.git"
  },
  "keywords": [
    "Photo",
    "viewer"
  ],
  "author": "h_n",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hn999/pictmgr/issues"
  },
  "homepage": "https://github.com/hn999/pictmgr#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "exif-reader": "^1.0.3",
    "express": "^4.17.1",
    "express-password-protect": "^1.0.2",
    "glob-ext": "^1.0.2",
    "image-thumbnail": "^1.0.13",
    "pako": "^2.0.3",
    "serve-index": "^1.9.1",
    "sharp": "^0.27.0",
    "simple-thumbnail": "^1.6.5"
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1"
  }
}
